
<!-- 1. Create Queue -->
<div id="1" class="menu-div" style="display:none">
    <form method="POST" action="/sqs-queue">
        <span>Create Queue</span><hr><br>
        <label>Queue Name:</label>
        <input placeholder="Queue Name" id="queuename" type="text" name="queuename" value="<%= ui.def_sqsname %>" required><br><br>
        <input type="submit" value="Create Queue"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[1] %></textarea>
    </form>
</div>

<!-- 2. List all Queues -->    
<div id="2" class="menu-div" style="display:none">
    <form  method="GET" action="/sqs-queue/list">
        <span>List Queues</span><hr><br><br>
        <input type="submit" value="List Queue URLs"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[2] %></textarea>
    </form>
</div>

<!-- 3. Get Queue Url -->    
<div id="3" class="menu-div" style="display:none">
    <form  method="GET" action="/sqs-queue">
        <span>Get Queue Url</span><hr><br>
        <label>Queue Name:</label>
        <input placeholder="Queue Name" id="queuename" type="text" name="queuename" value="<%= ui.def_sqsname %>"><br><br>
        <input type="submit" value="Get Queue URL"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[3] %></textarea>
    </form>
</div>

<!-- 4. Get Queue Attributes -->
<div id="4" class="menu-div" style="display:none">
    <form  method="GET" action="/sqs-queue/attributes">
        <span>Get Queue Attributes</span><hr><br>
        <label>Queue URL:</label>
        <input placeholder="Queue Url" id="queueurl" type="text" name="queueurl"value="<%= ui.def_sqsurl %>"required><br><br>
        <input type="submit" value="Get Queue Attributes"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[4] %></textarea>
    </form>
</div>

<!-- 5. Post Message to Queue -->    
<div id="5" class="menu-div" style="display:none">
    <form method="POST" action="/sqs-queue/message">
        <span>Post Message To Queue</span><hr><br>
        <label>Queue URL:</label>
        <input placeholder="Queue Url" id="queueurl" type="text" name="queueurl" value="<%= ui.def_sqsurl %>" required><br><br>
        <label>Message:</label>
        <textarea placeholder="Message" id="message" name="message" value="" rows="5" required></textarea><br><br>
        <input type="submit" value="Sent Message"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[5] %></textarea>
    </form>
</div>

<!--6. Get Message from Queue -->    
<div id="6" class="menu-div" style="display:none">
    <form method="GET" action="/sqs-queue/message">
        <span>Get Message From Queue</span><hr><br>
        <label>Queue URL:</label>
        <input placeholder="Queue Url" id="queueurl" type="text" name="queueurl" value="<%= ui.def_sqsurl %>" required><br><br>
        <input type="submit" value="Retrieve Message"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[6] %></textarea>
    </form>
</div>

<!-- 7. Delete Message from Queue -->    
<div id="7" class="menu-div" style="display:none">
    <form method="POST" action="/sqs-queue/message/delete">
    <span>Delete Message From Queue</span><hr><br>
        <label>Queue URL:</label>
        <input placeholder="Queue Url" id="queueurl" type="text" name="queueurl" value="<%= ui.def_sqsurl %>" required> <br><br>
        <label>Message Receipt Handle:</label>
        <input placeholder="Message Receipt Handle" id="messagehandle" type="text" name="messagehandle" value="<%= ui.def_msghandle %>" required><br><br>
        <input type="submit" value="Delete Message"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[7] %></textarea>
    </form>
</div>

<!-- 8. Purge Queue (delete all messsages) -->    
<div id="8" class="menu-div" style="display:none">
    <form method="POST" action="/sqs-queue/purge">
    <span>Purge Messages From Queue</span><hr><br>
        <label>Queue URL:</label>
        <input placeholder="Queue Url" id="queueurl" type="text" name="queueurl" value="<%= ui.def_sqsurl %>" required><br><br>
        <input type="submit" value="PURGE Queue"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[8] %></textarea>
    </form>
</div>

<!-- 9. Delete Queue (deletes queue and all messsages) --> 
<div id="9" class="menu-div" style="display:none">   
    <form method="POST" action="/sqs-queue/delete">
        <span>Delete Queue</span><hr><br>
        <label>Queue URL:</label>
        <input placeholder="Queue Url" id="queueurl" type="text" name="queueurl" value="<%= ui.def_sqsurl %>" required><br><br>
        <input type="submit" value="DELETE Queue"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[9] %></textarea>
    </form>
</div>

<!-- 10. Set queue attributes  -->
<div id="10" class="menu-div" style="display:none">
    <form method="POST" action="/sqs/setqattr">
        <span>Set Queue Policy</span><hr><br>
        <label>Queue URL:</label>
        <input placeholder="SQS Queue Url" id ="sqsurl" type="text" name="sqsurl" value="<%= ui.def_sqsurl %>" required><br><br>
        <label>Queue ARN:</label>
        <input placeholder="SQS Queue Arn" id ="sqsarn" type="text" name="sqsarn" value="<%= ui.def_sqsarn %>" required><br><br>
        <label>Topic ARN:</label>
        <input placeholder="SNS Topic Arn" id ="snsarn" type="text" name="snsarn" value="<%= ui.def_snsarn %>" required><br><br>
        <input type="submit" value="Set Queue Policy"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[10] %></textarea>
    </form>
</div>